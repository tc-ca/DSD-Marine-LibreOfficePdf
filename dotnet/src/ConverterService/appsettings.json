{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FileUpload": {
    "MultipartBoundaryLengthLimit": 128,
    "PermittedExtensions": [ ".txt", ".docx", ".xlsx", ".pptx", ".pdf", ".fdf", ".png", ".jpg", ".jpeg", ".gif" ],
    "RestrictedCharacters": "\"*:<>?/\\{|}~?%$#&!^()[]`'@,;",
    "FileNameLengthLimit": 128,
    "MaxRequestBodySize": 52428800,
    "FileSizeLimit": 8388608,
    "ReadBufferSize": 4096,
    "WriteBufferSize": 4096,
    "ValidateFileSignatures": true,
    "FileSignatures": {
      ".gif": [ "R0lGOA==" ],
      ".png": [ "iVBORw0KGgo=" ],
      ".jpeg": [ "/9j/4A==", "/9j/4g==", "/9j/4w==" ],
      ".jpg": [ "/9j/4A==", "/9j/4Q==", "/9j/6A==" ],
      ".pdf": [ "JVBERg==" ],
      ".fdf": [ "JUZERg==" ],
      ".docx": [ "UEsDBA==", "UEsDBBQABgA=" ],
      ".xlsx": [ "UEsDBA==", "UEsDBBQABgA=" ],
      ".pptx": [ "UEsDBA==", "UEsDBBQABgA=" ]
    }
  },
  "Conversion": {
    "ConvertToPdfCommandTemplate": "libreoffice --headless --invisible --convert-to pdf \"{0}\" --outdir \"{1}\"",
    "ConvertToPdfTimeout": 30000,
    "MergePdfCommandTemplate": "gs -dBATCH -dNOPAUSE -q -sDEVICE=pdfwrite -dAutoRotatePages=/None -sOutputFile=\"{0}\" {1}",
    "MergedFileName": "merged.pdf",
    "MergePdfTimeout": 30000,
    "FillPdfFormCommandTemplate": "pdftk \"{0}\" fill_form \"{1}\" output \"{2}\" flatten",
    "FillPdfFormTimeout": 30000,
    "FilledFormFileName": "completed.pdf",
    "GenerateFdfCommandTemplate": "pdftk \"{0}\" generate_fdf output \"{1}\"",
    "GenerateFdfTimeout": 30000,
    "GeneratedFdfFileName": "template.fdf"
  }
}
